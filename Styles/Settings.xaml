<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:sys="clr-namespace:System;assembly=mscorlib">
	<Color x:Key="PrimaryBackgroundColor">#FF222222</Color>
	<Color x:Key="PrimaryColor">#FF6DB6F2</Color>
	<Color x:Key="SecondaryColor">#FFFFFFFF</Color>
	<Color x:Key="ButtonColor">#FF444444</Color>
	<Color x:Key="ButtonOverColor">#FF555555</Color>
	<Color x:Key="ButtonDownColor">#FF333333</Color>

	<SolidColorBrush x:Key="PrimaryBackgroundBrush" Color="{StaticResource PrimaryBackgroundColor}"/>
	<SolidColorBrush x:Key="PrimaryBrush" Color="{StaticResource PrimaryColor}"/>
	<SolidColorBrush x:Key="SecondaryBrush" Color="{StaticResource SecondaryColor}"/>

	<SolidColorBrush x:Key="ButtonBrush" Color="{StaticResource ButtonColor}"/>
	<SolidColorBrush x:Key="ButtonOverBrush" Color="{StaticResource ButtonOverColor}"/>
	<SolidColorBrush x:Key="ButtonDownBrush" Color="{StaticResource ButtonDownColor}"/>

	<sys:Double x:Key="InputFontSize">15</sys:Double>
	<sys:Double x:Key="HeaderFontSize">25</sys:Double>
	<sys:Double x:Key="SmallButtonSize">20</sys:Double>

	<sys:Double x:Key="LargeButtonSize">45</sys:Double>
	<GridLength x:Key="LargeButtonGridSize">45</GridLength>

	<sys:Double x:Key="TabHeight">30</sys:Double>
	
	<GridLength x:Key="IOBoxWidth">200</GridLength>
	<sys:Double x:Key="IOBoxWidthMinusOne">199</sys:Double>
</ResourceDictionary>